{
  "name": "@microsoft/signalr-protocol-msgpack",
  "version": "5.0.0-dev",
  "description": "MsgPack Protocol support for ASP.NET Core SignalR",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "typings": "./dist/esm/src/index.d.ts",
  "umd": "./dist/browser/signalr-protocol-msgpack.js",
  "umd_name": "signalR.protocols.msgpack",
  "unpkg": "./dist/browser/signalr-protocol-msgpack.js",
  "directories": {
    "test": "spec"
  },
  "sideEffects": false,
  "scripts": {
    "preclean": "cd ../common && npm install --mutex network --frozen-lockfile",
    "clean": "npx rimraf ./dist",
    "prebuild": "npm run clean && npm install --mutex network --frozen-lockfile",
    "build": "npm run build:esm && npm run build:cjs && npm run build:browser && npm run build:uglify",
    "build:esm": "npx tsc --project ./tsconfig.json --module es2015 --outDir ./dist/esm -d",
    "build:cjs": "npx tsc --project ./tsconfig.json --module commonjs --outDir ./dist/cjs",
    "build:browser": "npx webpack",
    "build:uglify": "npx terser -m -c --ecma 2019 --module --source-map \"url='signalr-protocol-msgpack.min.js.map',content='./dist/browser/signalr-protocol-msgpack.js.map'\" --comments -o ./dist/browser/signalr-protocol-msgpack.min.js ./dist/browser/signalr-protocol-msgpack.js",
    "prepack": "node ../build/embed-version.js",
    "test": "echo \"Run 'npm test' in the 'clients/ts' folder to test this package\" && exit 1"
  },
  "keywords": [
    "signalr",
    "aspnetcore"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotnet/aspnetcore.git"
  },
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotnet/aspnetcore/issues"
  },
  "homepage": "https://github.com/dotnet/aspnetcore/tree/main/src/SignalR#readme",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@microsoft/signalr": "*",
    "@msgpack/msgpack": "^2.7.0",
    "network": "^0.6.1"
  },
  "resolutions": {
    "ws": ">=7.4.6"
  }
}
